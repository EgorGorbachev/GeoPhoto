<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.example.geophoto.ui.splash.SplashScreenFragment"
        android:label="fragment_loading_screen"
        tools:layout="@layout/fragment_splash_screen">
        <action
            android:id="@+id/action_loadingScreenFr_to_galleryFr"
            app:destination="@id/galleryFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/from_right" />
        <action
            android:id="@+id/action_loadingScreenFr_to_authenticationFr"
            app:destination="@id/authenticationFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/galleryFr"
        android:name="com.example.geophoto.ui.photo.PhotoFragment"
        android:label="fragment_gallery"
        tools:layout="@layout/fragment_photo">
        <action
            android:id="@+id/action_galleryFr_to_settingsFr"
            app:destination="@id/settingsFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_galleryFr_to_photoGalleryFr"
            app:destination="@id/photoGalleryFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/from_right" />
    </fragment>
    <fragment
        android:id="@+id/settingsFr"
        android:name="com.example.geophoto.ui.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFr_to_galleryFr"
            app:destination="@id/galleryFr"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/photoGalleryFr"
        android:name="com.example.geophoto.ui.photo_gallery.PhotoGalleryFragment"
        android:label="PhotoGalleryFr"
        tools:layout="@layout/fragment_photo_gallery">
        <action
            android:id="@+id/action_photoGalleryFr_to_photoGalleryFr"
            app:destination="@id/galleryFr"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_photoGalleryFr_to_photoDetailsFr"
            app:destination="@id/photoDetailsFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/photoDetailsFr"
        android:name="com.example.geophoto.ui.photo_details.PhotoDetailsFragment"
        android:label="fragment_photo_details"
        tools:layout="@layout/fragment_photo_details">
        <action
            android:id="@+id/action_photoDetailsFr_to_photoGalleryFr"
            app:destination="@id/photoGalleryFr"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
        <action
            android:id="@+id/action_photoDetailsFr_to_mapFragment"
            app:destination="@id/mapFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <argument
            android:name="PhotoLocation"
            app:argType="com.example.domain.model.PhotoLocation" />
    </fragment>
    <fragment
        android:id="@+id/authenticationFr"
        android:name="com.example.geophoto.ui.authentication.AuthenticationFragment"
        android:label="fragment_authentication"
        tools:layout="@layout/fragment_authentication" >
        <action
            android:id="@+id/action_authenticationFr_to_registrationFr"
            app:destination="@id/registrationFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_authenticationFr_to_galleryFr"
            app:destination="@id/galleryFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_authenticationFr_to_forgotPasswordFr"
            app:destination="@id/forgotPasswordFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/registrationFr"
        android:name="com.example.geophoto.ui.registration.RegistrationFragment"
        android:label="fragment_registration"
        tools:layout="@layout/fragment_registration" >
        <action
            android:id="@+id/action_registrationFr_to_galleryFr"
            app:destination="@id/galleryFr"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_registrationFr_to_authenticationFr"
            app:destination="@id/authenticationFr"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordFr"
        android:name="com.example.geophoto.ui.forgot_password.ForgotPasswordFragment"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" >
        <action
            android:id="@+id/action_forgotPasswordFr_to_authenticationFr"
            app:destination="@id/authenticationFr"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left"/>
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.geophoto.ui.map.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" >
        <action
            android:id="@+id/action_mapFragment_to_photoDetailsFr"
            app:destination="@id/photoDetailsFr"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_right"
            app:popExitAnim="@anim/to_left"/>
        <argument
            android:name="PhotoLatLng"
            app:argType="com.example.data.model.pojo.PhotoLatLng" />
        <argument
            android:name="PhotoLocation"
            app:argType="com.example.domain.model.PhotoLocation" />
    </fragment>
</navigation>